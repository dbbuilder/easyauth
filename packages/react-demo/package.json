{
  "name": "@easyauth/react-demo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "React demo application for EasyAuth Framework",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "mock-server": "node mock-server.cjs",
    "dev:full": "concurrently \"npm run mock-server\" \"npm run dev\" --names \"server,client\" --prefix-colors \"blue,green\""
  },
  "dependencies": {
    "@easyauth/react": "file:../react",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.2.1",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "jsdom": "^23.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
